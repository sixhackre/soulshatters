local char = game.Players.LocalPlayer.Character
if not char:FindFirstChild("OverwriteButton") then
			local button = game.ReplicatedStorage.Effects.OverwriteButton:Clone()
			button:SetPrimaryPartCFrame(char.HumanoidRootPart.CFrame)
			local weld = Instance.new("WeldConstraint")
			weld.Part0 = button.HumanoidRootPart
			weld.Part1 = char.HumanoidRootPart
			weld.Parent = button
			button.Parent = char
			local anim = button.AnimationController:LoadAnimation(button.Idle2)
			anim:Play()
			button.OverwriteButton.Spawn:Play()
			button.OverwriteButton.Sound:Play()
			task.wait(.14)
			for i,v in pairs(button.OverwriteButton:GetChildren()) do
				if v:IsA("ParticleEmitter") then
					v.Enabled = false
				end
			end
			button.OverwriteButton.Color = Color3.fromRGB(255,0,0)
			for i,v in pairs(button:GetDescendants()) do
				if v:IsA("ParticleEmitter") then
					v.Color = ColorSequence.new(Color3.new(1,0,0))
				elseif v:IsA("PointLight") then
					v.Color = Color3.fromRGB(100,0,0)
				elseif v:IsA("ImageLabel") then
					v.Image = "rbxassetid://8286566767"
				end
				end
			end

--

