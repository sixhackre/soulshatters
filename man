--//Tp\\

function funnysoundeffect()
--6920047468
local SE = Instance.new("Sound",game.Players.LocalPlayer.Character["Throw Away Effects"])
SE.Volume = 3
SE.Name = "SoundEffect"
SE.PlaybackSpeed = 1
SE.SoundId = "rbxassetid://8627516764"
SE.Looped = false
SE:Play()
end

local uis = game:GetService("UserInputService")
local player = game.Players.LocalPlayer

uis.InputBegan:Connect(function(inputs, event)
	if event == true then return end

	if inputs.KeyCode == Enum.KeyCode.R and InAction == false then

		if player.Backpack:WaitForChild("Main").LockOnScript.LockOn.Value == nil then


			local cfr = player:GetMouse().Hit

			player.Character:SetPrimaryPartCFrame(cfr * CFrame.new(0, 2, 0))
                char = game.Players.LocalPlayer.Character for _,v in pairs(game:GetService("ReplicatedStorage").Resources.LocalScripts:GetChildren()) do if v.Name == 'GlitchScreen3' then clone = v:Clone() clone.Parent = char end end
            funnysoundeffect()
			wait(0.5)
			            game.Players.LocalPlayer.Character["Throw Away Effects"]:ClearAllChildren()

		else
			game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = getlockchar().HumanoidRootPart.CFrame * CFrame.new(0,0,-3)
                char = game.Players.LocalPlayer.Character for _,v in pairs(game:GetService("ReplicatedStorage").Resources.LocalScripts:GetChildren()) do if v.Name == 'GlitchScreen3' then clone = v:Clone() clone.Parent = char end end
            funnysoundeffect()
						wait(0.5)
			            game.Players.LocalPlayer.Character["Throw Away Effects"]:ClearAllChildren()

		end

	end

end)


--//Attacks\\
      local CAS = game:GetService("ContextActionService")
            local FREEZE_COMMAND = "CantAttack"
            CAS:BindActionAtPriority(
                FREEZE_COMMAND,
                function()
                    return Enum.ContextActionResult.Sink
                end,
                false,
                Enum.ContextActionPriority.High.Value,
                Enum.KeyCode.E,
                Enum.KeyCode.Q
                )
local pass = getrenv()._G.Pass

local debounce = true 
local cooldown = 7
local mouse = game.Players.LocalPlayer:GetMouse()
mouse.KeyDown:Connect(function(k, typing)
    if typing then return end
	if k == "1" and Player.Backpack.Main.LockOnScript.LockOn.Value ~= nil and InAction == false then

        local args = {
            [1] = {
            [1] = getrenv()._G.Pass,
            [2] = "Damage",
            [3] = math.huge,
            [4] = game.Players.LocalPlayer.Backpack.Main.LockOnScript.LockOn.Value
                    }
                }
                if game.Players.LocalPlayer.Backpack.Main.LockOnScript.LockOn.Value ~= nil then
            game:GetService("ReplicatedStorage").Remotes.Events:FireServer(unpack(args))
                end
           end
           end)


    local debounce = true 
local CD = 4
local Cooldown2 = false
local mouse = game.Players.LocalPlayer:GetMouse()
mouse.KeyDown:Connect(function(k, typing)
    if typing then return end
	if k == "2" and Player.Backpack.Main.LockOnScript.LockOn.Value ~= nil and Cooldown2 == false and InAction == false then

        local Anim = Instance.new("Animation")
        Anim.AnimationId = "rbxassetid://4361511573"
        local k = game.Players.LocalPlayer.Character.Humanoid:LoadAnimation(Anim)
        if game.Players.LocalPlayer.Backpack.Main.LockOnScript.LockOn.Value ~= nil then
k:Play()
end
