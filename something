local function castRay(p2)
	local v7 = nil;
	v7 = Ray.new(p2.Origin, p2.Direction);
	if p2.IgnoreList then
		return workspace:FindPartOnRayWithIgnoreList(v7, p2.IgnoreList);
	end;
	return workspace:FindPartOnRay(v7, p2.Model);
end;
local function CreateTween(p3, p4, p5, p6)
	local v8 = tweenS:Create(p3, TweenInfo.new(unpack(p4)), p5);
	if p6 then
		v8:Play();
	end;
	return v8;
end;
local function clonePart(p7, p8, p9)
	local v9 = p7:Clone();
	v9.CFrame = p9;
	v9.Parent = p8;
	return v9;
end;
