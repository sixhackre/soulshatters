local function removeParticles(p12, p13, p14)
	spawn(function()
		wait(p12);
		local v15, v16, v17 = pairs(p13:GetChildren());
		while true do
			local v18, v19 = v15(v16, v17);
			if v18 then

			else
				break;
			end;
			v17 = v18;
			if v19:IsA("ParticleEmitter") then
				v19.Enabled = false;
			end;		
		end;
		game.Debris:AddItem(p13, p14);
	end);
end;
local function enableParticles(p15, p16)
	spawn(function()
		local v20, v21, v22 = pairs(p15:GetChildren());
		while true do
			local v23, v24 = v20(v21, v22);
			if v23 then

			else
				break;
			end;
			v22 = v23;
			if v24:IsA("ParticleEmitter") then
				v24.Enabled = p16;
			end;		
		end;
	end);
end;
local function Lerp(p17, p18, p19)
	return p17 + (p18 - p17) * p19;
end;
local function checkIfMoving(p20)
	if not (0 < p20.MoveDirection.X) then
		if not (p20.MoveDirection.X < 0) then
			if not (0 < p20.MoveDirection.Z) then
				if p20.MoveDirection.Z < 0 then

				else
					return;
				end;
			end;
		end;
	end;
	return true;
end;
local function createAnim(id,name,parent)
	local anim = Instance.new("Animation")
	if name then
		anim.Name = name
	end
	anim.AnimationId = "rbxassetid://"..id
	if parent then anim.Parent = parent end
	return anim
end
