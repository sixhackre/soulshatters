if game.PlaceId == 3198259055 then
local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()

--//Main
local vis = false
local Window = Library.CreateLib("ExternalShatters", "Midnight")
local Tab1 = Window:NewTab("Custom")
local Tab2 = Window:NewTab("Fun Things")
local Tab3 = Window:NewTab("Settings")

local Section1 = Tab1:NewSection("Custom")
local Section2 = Tab2:NewSection("Commands")
local Section3 = Tab3:NewSection("Gui")
local Section4 = Tab1:NewSection("Paid Custom")



--//Buttons And Scripts

Section3:NewToggle("Toggle Visualizers In Scripts", "ToggleInfo", function(state)
    if state then
        print("Toggle On")
	local vis = true
    else
        print("Toggle Off")
	local vis = false
    end
end)

Section3:NewKeybind("Toggle Gui", "Toggle The Gui Lol", Enum.KeyCode.RightControl, function()
	Library:ToggleUI()
end)

Section1:NewButton("IDK Custom", "IDK Custom Kinda Buggy", function()
    warn("IDK Custom Loaded")
    loadstring(game:HttpGet("https://raw.githubusercontent.com/sixhackre/soulshatters/main/idthinglol3", true))()
end)

Section1:NewButton("Holy Custom", "Holy Custom", function()
    warn("Holy Custom Loaded")
    --made by h01y
    loadstring(game:HttpGet("https://raw.githubusercontent.com/sixhackre/soulshatters/main/holy", true))()
end)

Section1:NewButton("Holy Custom", "Holy Custom", function()
if vis == true then
    warn("DripGokuVisLoaded")
    loadstring(game:HttpGet("https://raw.githubusercontent.com/sixhackre/soulshatters/main/DripGokuV10Vis", true))()
    end
    if vis == false then
    warn("DripGokuNoVisLoaded")
    loadstring(game:HttpGet("https://raw.githubusercontent.com/sixhackre/soulshatters/main/GokuDripV10NoVis", true))()
    end
end)

Section1:NewButton("Holy Custom", "Holy Custom", function()
    warn("Holy Custom Loaded")
        warn("TheTrueGodV7VisLoaded")
    loadstring(game:HttpGet("https://raw.githubusercontent.com/sixhackre/soulshatters/main/TheTrueGodV7Vis", true))()
    end
    if vis == false then
    warn("TheTrueGodV7NoVisLoaded")
    loadstring(game:HttpGet("https://raw.githubusercontent.com/sixhackre/soulshatters/main/TheTrueGodV7NoVis", true))()
    end
end)


--//Section2

Section2:NewButton("Frisk Level 20", "Level 20 Frisk", function()
    warn("Frisk Level20 Loaded")
    loadstring(game:HttpGet("https://raw.githubusercontent.com/miguel831/Topi/main/SoulShattersScripts/Frisk%20Level%2020", true))()
end)


--//Section4

Section4:NewButton("Demi God", "OP AF", function()
    warn("Demi God Loaded")
    loadstring( game:HttpGet("https://raw.githubusercontent.com/miguel831/Topi/main/SoulShattersScripts/Demi%20god", true))()
end)

Section4:NewButton("Last Breath", "Dialogs..", function()
    warn("Last Breath Loaded")
    loadstring(game:HttpGet("https://raw.githubusercontent.com/miguel831/Topi/main/SoulShattersScripts/LastBreath", true))()
end)

Section4:NewButton("Fatal Error Sans", "Fun", function()
    warn("Fatal Error Sans Loaded")
    loadstring(game:HttpGet("https://raw.githubusercontent.com/miguel831/Topi/main/SoulShattersScripts/LastBreath", true))()
end)
end
